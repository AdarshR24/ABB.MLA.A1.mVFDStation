<UserControl x:Class="ABB.MLA.A1.mVFDStation.UserControls.InitUc"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ABB.MLA.A1.mVFDStation.UserControls"
             mc:Ignorable="d" x:Name="root"
             Height="Auto" Width="Auto">
    <Grid>
        <Button Height="20" Width="60"
                Command="{Binding BtnCommand, ElementName=root}"
                Content="{Binding ButtonContent,ElementName=root}"
           Style="{StaticResource Button_all}"></Button>
    </Grid>
</UserControl>
